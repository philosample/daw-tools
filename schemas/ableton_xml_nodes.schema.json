      "enum": ["ableton_doc", "ableton_artifact"]
    },
    "scanned_at": {"type": "integer"},
    "ord": {"type": "integer"},
    "depth": {"type": "integer"},
    "tag": {"type": "string"},
    "path_tag": {"type": "string"},
    "attrs": {"type": "object"},
    "text": {"type": "string"},
    "text_len": {"type": "integer"},
    "text_truncated": {"type": "boolean"}
  },
  "additionalProperties": true
}
